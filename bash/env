    . /usr/local/bin/virtualenvwrapper.sh
fi

if [ -f ~/.appomni ]; then
    . ~/.appomni
fi

. ~/bin/dotfiles/bash/vendor/git-completion.bash
. ~/bin/dotfiles/bash/vendor/git-prompt.sh
. ~/bin/dotfiles/bash/vendor/z.sh
